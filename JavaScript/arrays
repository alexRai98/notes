function findUniq(arr) {
  let b = arr.reduce((gen, value)=>{
    let a = arr.reduce((acum, val) => {  value === val ? acum +=1 :acum+=0 ; return acum},0) 
    gen[a]=value; return gen
  },{});
  return b[1]
}



